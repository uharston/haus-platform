{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"types": ["@sanity/astro/module"],
		"strictNullChecks": true,
		"baseUrl": "src",
		"paths": {
			"@/*": ["*"],
			"@lib/*": ["lib/*"],
			"@components/*": ["components/*"],
			"@layouts/*": ["layouts/*"],
			"@styles/*": ["styles/*"],
			"@utils/*": ["utils/*"],
			"@pages/*": ["pages/*"],
			"@assets/*": ["assets/*"],
			"@locales/*": ["locales/*"],
			"@i18n/*": ["i18n/*"],
		},
	},
}
